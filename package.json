{
  "name": "my-lib",
  "version": "0.0.0",
  "repository": "https://github.com/bitlogic-eng/ng-ui-lib.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:sc": "ng serve --project showcase-ui-lib",
    "start:e": "ng serve --project customElements",
    "build": "ng build",
    "build:lib": "ng build ui-lib --configuration production",
    "build:sc": "ng build --project showcase-ui-lib",
    "build:e": "ng build --configuration production --project customElements --output-hashing=none && node bundle-element.js custom-elements",
    "watch": "ng build --watch --configuration development",
    "watch:lib": "ng build ui-lib --watch --configuration development",
    "watch:lib:prod": "ng build ui-lib --watch --configuration production",
    "test": "ng test",
    "publish": "npm pack ./dist/ng-ui-lib && npm publish ./dist/ng-ui-lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.1",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/elements": "^14.0.1",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.0.1",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "fuse.js": "^6.6.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "concat": "^1.0.3",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.0",
    "typescript": "~4.7.2"
  }
}
